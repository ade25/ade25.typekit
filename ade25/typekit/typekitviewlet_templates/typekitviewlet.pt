<tal:extra-head omit-tag="" condition="viewlet/available">
    <script type="text/javascript" src="//use.typekit.net/lvq3qav.js"
        tal:attributes="src viewlet/kit_source_url">
    </script>
    <script type="text/javascript">
        try{Typekit.load({
            loading: function() {
              // Javascript to execute when fonts start loading
            },
            active: function() {
                if (jQuery.support.opacity) {
                    $('#appui-wf').css('visibility', 'visible').hide().fadeIn();
                } else {
                    $('#appui-wf').css('visibility', 'visible');
                }
            },
            inactive: function() {
                $('#appui-wf').css('visibility', 'visible');
            }
        });}catch(e){}</script>
</tal:extra-head>